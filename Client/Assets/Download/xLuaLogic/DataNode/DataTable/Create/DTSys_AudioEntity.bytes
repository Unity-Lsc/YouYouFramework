DTSys_AudioEntity = { Id = 0, Desc = "", AssetPath = "", Is3D = 0, Volume = 0 }

DTSys_AudioEntity.__index = DTSys_AudioEntity;

function DTSys_AudioEntity.New(Id, Desc, AssetPath, Is3D, Volume)
    local self = { };
    setmetatable(self, DTSys_AudioEntity);

    self.Id = Id;
    self.Desc = Desc;
    self.AssetPath = AssetPath;
    self.Is3D = Is3D;
    self.Volume = Volume;

    return self;
end