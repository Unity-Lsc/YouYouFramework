DTSysAudioEntity = { Id = 0, Desc = "", AssetPath = "", Is3D = 0, Volume = 0 }

DTSysAudioEntity.__index = DTSysAudioEntity;

function DTSysAudioEntity.New(Id, Desc, AssetPath, Is3D, Volume)
    local self = { };
    setmetatable(self, DTSysAudioEntity);

    self.Id = Id;
    self.Desc = Desc;
    self.AssetPath = AssetPath;
    self.Is3D = Is3D;
    self.Volume = Volume;

    return self;
end